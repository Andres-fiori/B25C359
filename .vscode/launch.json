{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run 307_Books_Laboratorio-2",
      "runtimeExecutable": "cds",
      "request": "launch",
      "killBehavior": "polite",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/307_Books_Laboratorio",
      "preLaunchTask": "dev-project-cap",
      "envFile": "${workspaceFolder}/307_Books_Laboratorio/env/.env2",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/307_Books_Laboratorio\\\",\\\"profileName\\\":\\\"\\\"}\"}"
      }
    },
    {
        "name": "lrp-config1",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/lrp",
        "runtimeExecutable": "npx",
        "windows": {
            "runtimeExecutable": "npx.cmd"
        },
        "runtimeArgs": [
            "fiori",
            "run"
        ],
        "args": [
            "--open",
            "index.html"
        ],
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
            "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/lrp\"}",
            "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/v2/northwind\",\"url\":\"https://services.odata.org\",\"destination\":\"Northwind\"}]",
            "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
        }
    },
    {
        "name": "Start lrp",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/lrp",
        "runtimeExecutable": "npx",
        "windows": {
            "runtimeExecutable": "npx.cmd"
        },
        "runtimeArgs": [
            "fiori",
            "run"
        ],
        "args": [
            "--open",
            "test/flp.html#app-preview"
        ],
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
            "DEBUG": "--inspect",
            "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
            "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/lrp\"}"
        }
    },
    {
